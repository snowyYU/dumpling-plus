{
  "name": "dumpling-plus",
  "version": "0.2.18",
  "description": "vue3 dumpling plus",
  "private": false,
  "main": "lib/index.umd.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "serve": "vue-cli-service serve",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "npm run build:clean && npm run build:lib && npm run build:esm-bundle && rimraf lib/demo.html",
    "build:clean": "rimraf lib",
    "build:lib": "vue-cli-service build --target lib --name index --dest lib packages/index.ts",
    "build:rollup-umd": "npm run build:clean && rollup --config ./build/rollup.config.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "core-js": "^3.21.1",
    "dayjs": "^1.10.7",
    "dumpling-plus": "^0.2.1",
    "element-plus": "^2.0.2",
    "js-cookie": "^3.0.1",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "path-browserify": "^0.0.1",
    "regenerator-runtime": "^0.13.9",
    "register-service-worker": "^1.7.2",
    "screenfull": "^5.2.0",
    "vue": "^3.2.31",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-env": "^7.17.12",
    "@mdx-js/react": "1.6.22",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.5.0-rc.0",
    "@storybook/addon-backgrounds": "6.5.0-alpha.61",
    "@storybook/addon-docs": "^6.5.0-rc.0",
    "@storybook/addon-essentials": "^6.5.0-rc.0",
    "@storybook/addon-links": "^6.5.0-rc.0",
    "@storybook/addon-measure": "6.5.0-alpha.61",
    "@storybook/addon-outline": "6.5.0-alpha.61",
    "@storybook/builder-vite": "^0.1.33",
    "@storybook/client-api": "6.5.0-alpha.61",
    "@storybook/client-logger": "6.5.0-alpha.61",
    "@storybook/core-common": "6.5.0-alpha.61",
    "@storybook/node-logger": "6.5.0-alpha.61",
    "@storybook/vue3": "6.5.0-alpha.61",
