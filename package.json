    "typescript": "~4.1.6",
    "webpackbar": "^5.0.2"
  },
  "peerDependencies": {
    "element-plus": "^2.0.2",
    "js-file-download": "^0.4.12",
    "lodash": "^4.17.21",
    "vue": "^3.2.31"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
